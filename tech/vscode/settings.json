{
  "editor.codeLens": false,
  "editor.fontFamily": "'Source Han Code JP'",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.minimap.showSlider": "always",
  "editor.scrollBeyondLastLine": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/node_modules": true
  },
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "html.format.wrapLineLength": 0,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Simple Light",
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "simple-icon",
  "editor.suggestSelection": "first",
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.tabSize": 2
  },
  "editor.columnSelection": false,
  "terminal.integrated.fontFamily": "Source Han Code JP",
  "extensions.closeExtensionDetailsOnViewChange": true,
  "git.ignoreMissingGitWarning": true,
  "remote.SSH.configFile": "C:\\Users\\xyz\\.ssh\\config",
  "remote.SSH.remotePlatform": {
    "emp.euphonic": "linux",
    "stg.euphonic": "linux",
    "dev.euphonic": "linux",
    "cube": "linux"
  },
  "window.restoreWindows": "none",
  "workbench.startupEditor": "none",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "terminal.integrated.fontWeightBold": "normal",
  "emmet.variables": {
    "lang": "ja"
  },
  "eslint.alwaysShowStatus": true,
  "files.eol": "\n",
  "liveServer.settings.https": {
    "enable": false,
    "cert": "/home/x/.ssh/cert.pem",
    "key": "/home/x/.ssh/private.key",
    "passphrase": ""
  },
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "terminal.background": "#263238",
    "terminal.foreground": "#EEFFFF",
    "terminalCursor.background": "#EEFFFF",
    "terminalCursor.foreground": "#EEFFFF",
    "terminal.ansiBlack": "#263238",
    "terminal.ansiBlue": "#82AAFF",
    "terminal.ansiBrightBlack": "#A8A8A8",
    "terminal.ansiBrightBlue": "#82AAFF",
    "terminal.ansiBrightCyan": "#89DDFF",
    "terminal.ansiBrightGreen": "#C3E88D",
    "terminal.ansiBrightMagenta": "#C792EA",
    "terminal.ansiBrightRed": "#F07178",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#FFCB6B",
    "terminal.ansiCyan": "#89DDFF",
    "terminal.ansiGreen": "#C3E88D",
    "terminal.ansiMagenta": "#C792EA",
    "terminal.ansiRed": "#F07178",
    "terminal.ansiWhite": "#EEFFFF",
    "terminal.ansiYellow": "#FFCB6B",
    "terminal.selectionBackground": "#666666"
  },
  "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "terminal.integrated.tabs.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.unicodeHighlight.nonBasicASCII": false,
}